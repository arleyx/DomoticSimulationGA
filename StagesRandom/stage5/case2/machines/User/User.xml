	    		<assign name="&id;state" expr="random.state(4)"/>
			</onentry>
			<transition event="&id;decidePiso1-patio" cond="&id;state eq 0" target="&id;Piso1-patio"/>
			<transition event="&id;decidePiso1-patio" cond="&id;state eq 1" target="&id;Piso1-sala"/>
			<transition event="&id;decidePiso1-patio" cond="&id;state eq 2" target="&id;Piso1-patio">
<if cond="states.get('bombillo9') eq 'offState'">
