<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE scxml SYSTEM "template.dtd">
<!-- @device=User -->
<scxml xmlns="http://www.w3.org/2005/07/scxml" version="1.0">
	<state id="&id;userMachine">
		<datamodel>
			<data name="&id;state" expr="''"/>
		</datamodel>
		<initial>
			<transition target="&id;Afuera"/>
		</initial>
			<state id="&id;Afuera">
	    	<onentry>
	    		<send sendid="&id;decideAfuera" targettype="'scxml'" event="'&id;decideAfuera'" delay="random.time()"/>
	    		<assign name="&id;state" expr="random.state(2)"/>
			</onentry>
			<transition event="&id;decideAfuera" cond="&id;state eq 0" target="&id;Afuera"/>
			<transition event="&id;decideAfuera" cond="&id;state eq 1" target="&id;Piso1-sala"/>
	    	<onexit>
				<cancel sendid="&id;decideAfuera"/>
			</onexit>
	    </state>
			<state id="&id;Piso1-bano1">
	    	<onentry>
	    		<send sendid="&id;decidePiso1-bano1" targettype="'scxml'" event="'&id;decidePiso1-bano1'" delay="random.time()"/>
	    		<assign name="&id;state" expr="random.state(3)"/>
			</onentry>
			<transition event="&id;decidePiso1-bano1" cond="&id;state eq 0" target="&id;Piso1-bano1"/>
			<transition event="&id;decidePiso1-bano1" cond="&id;state eq 1" target="&id;Piso1-sala"/>
			<transition event="&id;decidePiso1-bano1" cond="&id;state eq 2" target="&id;Piso1-bano1">
<if cond="states.get('bombillo5') eq 'offState'">
	<send targettype="'scxml'" event="'&id;bombillo5_on'"/>
<else/>
	<send targettype="'scxml'" event="'&id;bombillo5_off'"/>
</if>
			</transition>
	    	<onexit>
				<cancel sendid="&id;decidePiso1-bano1"/>
			</onexit>
	    </state>
			<state id="&id;Piso1-bano2">
	    	<onentry>
	    		<send sendid="&id;decidePiso1-bano2" targettype="'scxml'" event="'&id;decidePiso1-bano2'" delay="random.time()"/>
	    		<assign name="&id;state" expr="random.state(2)"/>
			</onentry>
			<transition event="&id;decidePiso1-bano2" cond="&id;state eq 0" target="&id;Piso1-bano2"/>
			<transition event="&id;decidePiso1-bano2" cond="&id;state eq 1" target="&id;Piso1-sala"/>
