			<transition event="&id;decidePiso1-sala" cond="&id;state eq 0" target="&id;Piso1-sala"/>
			<transition event="&id;decidePiso1-sala" cond="&id;state eq 1" target="&id;Afuera"/>
			<transition event="&id;decidePiso1-sala" cond="&id;state eq 2" target="&id;Piso1-cocina"/>
