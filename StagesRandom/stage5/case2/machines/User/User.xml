	    		<assign name="&id;state" expr="random.state(5)"/>
			</onentry>
			<transition event="&id;decidePiso1-cocina" cond="&id;state eq 0" target="&id;Piso1-cocina"/>
			<transition event="&id;decidePiso1-cocina" cond="&id;state eq 1" target="&id;Piso1-sala"/>
			<transition event="&id;decidePiso1-cocina" cond="&id;state eq 2" target="&id;Piso1-cocina">
<if cond="states.get('bombillo1') eq 'offState'">
	<send targettype="'scxml'" event="'&id;bombillo1_on'"/>
<else/>
	<send targettype="'scxml'" event="'&id;bombillo1_off'"/>
</if>
			</transition>
			<transition event="&id;decidePiso1-cocina" cond="&id;state eq 3" target="&id;Piso1-cocina">
<if cond="states.get('microondas1') eq 'onlineState'">
	<if cond="random.state(2) eq 0">
		<send targettype="'scxml'" event="'&id;microondas1_open'"/>
	<else/>
		<send targettype="'scxml'" event="'&id;microondas1_close'"/>
	</if>
<elseif cond="states.get('microondas1') eq 'openState'"/>
	<send targettype="'scxml'" event="'&id;microondas1_close'"/>
<elseif cond="states.get('microondas1') eq 'closeState'"/>
	<if cond="random.state(2) eq 0">
		<send targettype="'scxml'" event="'&id;microondas1_open'"/>
	<else/>
		<send targettype="'scxml'" event="'&id;microondas1_offline'"/>
	</if>
<else/>
	<send targettype="'scxml'" event="'&id;microondas1_online'"/>
</if>
			</transition>
			<transition event="&id;decidePiso1-cocina" cond="&id;state eq 4" target="&id;Piso1-cocina">
<if cond="states.get('licuadora1') eq 'onlineState'">
