	</if>
<else/>
	<send targettype="'scxml'" event="'&id;equipo1_online'"/>
</if>
			</transition>
	    	<onexit>
				<cancel sendid="&id;decidePiso1-habitacion1"/>
			</onexit>
	    </state>
			<state id="&id;Piso1-habitacion2">
	    	<onentry>
