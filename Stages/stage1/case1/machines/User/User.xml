</if>
			</transition>
			<transition event="&id;decidePiso2-Estudio" cond="&id;state eq 4" target="&id;Piso2-Estudio">
<if cond="states.get('cargador1') eq 'offlineState'">
	<send targettype="'scxml'" event="'&id;cargador1_online'"/>
<else/>
	<send targettype="'scxml'" event="'&id;cargador1_offline'"/>
</if>
			</transition>
	    	<onexit>
				<cancel sendid="&id;decidePiso2-Estudio"/>
			</onexit>
	    </state>
