<else/>
	<send targettype="'scxml'" event="'&id;bombillo1_off'"/>
</if>
			</transition>
			<transition event="&id;decidePiso1-Cocina" cond="&id;state eq 4" target="&id;Piso1-Cocina">
<if cond="states.get('nevera1') eq 'onlineState'">
	<if cond="random.state(2) eq 0">
		<send targettype="'scxml'" event="'&id;nevera1_open'"/>
	<else/>
		<send targettype="'scxml'" event="'&id;nevera1_close'"/>
	</if>
<elseif cond="states.get('nevera1') eq 'openState'"/>
	<send targettype="'scxml'" event="'&id;nevera1_close'"/>
<elseif cond="states.get('nevera1') eq 'closeState'"/>
