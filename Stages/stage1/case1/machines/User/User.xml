			<transition event="&id;decidePiso2-Habitacion1" cond="&id;state eq 4" target="&id;Piso3-Habitacion2"/>
			<transition event="&id;decidePiso2-Habitacion1" cond="&id;state eq 5" target="&id;Piso2-Habitacion1">
<if cond="states.get('bombillo6') eq 'offState'">
	<send targettype="'scxml'" event="'&id;bombillo6_on'"/>
<else/>
